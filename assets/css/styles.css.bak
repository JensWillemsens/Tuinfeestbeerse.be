}

.slider-content h2 span:before {
    position: absolute;
    right: 0;
    top: -5px;
    z-index: -9;
    border-left: 80px solid transparent;
    border-right: 80px solid transparent;
    border-top: 150px solid #f90048;
    content: "";
}

.slider-content p {
