}


/*-------header-area end-------*/


/*-------slider-area start-------*/

.slider-items {
    position: relative;
    z-index: 9;
}

.slider-items img {
    visibility: hidden;
}

.slider-items2:before {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    background: #f9004890;
    content: "";
    z-index: -9;
}

.slider-content {
    position: absolute;
    left: 0;
    top: 0px;
    width: 100%;
    height: 100%;
